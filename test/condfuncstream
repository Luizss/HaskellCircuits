main (s : Stream Bit) (l : Stream Bit) : Stream Bit
  | now (1 : Nat 1) (s : Stream Bit) : Bit = g (s : Stream Bit) (k (l : Stream Bit) : Stream Bit) : Stream Bit
  | now (1 : Nat 2) (l : Stream Bit) : Bit = k (s : Stream Bit) : Stream Bit

g (s : Stream Bit) (l : Stream Bit) : Stream Bit = (l : Stream Bit)
k (s : Stream Bit) : Stream Bit = (s : Stream Bit)